function shuffle() {
  var arr = []
  var currentIndex = arr.length,  randomIndex;
  var inputA = prompt("banyak a")
  var inputB = prompt("banyak B")
  var a = parseInt(inputA)
  var b = parseInt(inputB)
  var arrA = []
  var arrB = []
    
  for(let i = 0; i < a; i++){
    arrA.push("a")
  }
  for(let i = 0; i < b; i++){
    arrB.push("b")
  }

  arr = arrA.concat(arrB)
  arr.sort(() => Math.random() - 0.5)
  
  return arr;
}

